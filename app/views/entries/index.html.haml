- if @entries.empty?
- else
  %table
    %thead
      %tr
        %th Name
        %th Hash
        %th % Complete
    %tbody
      - @entries.each do |entry|
        %tr
          %td= link_to entry.name, "/e/#{entry.code}"
          %td= link_to entry.code, "/e/#{entry.code}"
          %td= "#{entry.link_count[:whole]} / #{entry.link_count[:total]} (#{entry.percent_complete})"
          
= form_for Entry.new(code: 'QmQjoxqJn1xVhRNvXaLfWjXQFq2DCj3iDWKcPkYMbSyx6W') do |f|
  = f.label :code
  = f.text_field :code
